  "faq": {
    "general": {
      "title": "General Questions",
      "items": [
        {
          "id": "what_is_2gc",
          "question": "What is 2GC-CloudBridge?",
          "answer": "2GC-CloudBridge is a secure solution for remote server access via RDP and SSH protocols. It provides secure connections and convenient connection management."
        },
        {
          "id": "how_to_connect",
          "question": "How to connect to a server?",
          "answer": "Select the desired server from the list, click the connect button, enter your credentials (username and password), and click 'Connect'."
        },
        {
          "id": "save_credentials",
          "question": "Is it safe to save credentials?",
          "answer": "Yes, all credentials are encrypted and stored locally on your computer. We use modern encryption methods to ensure security."
        }
      ]
    },
    "technical": {
      "title": "Technical Questions",
      "items": [
        {
          "id": "rdp_connection",
          "question": "How to configure RDP connection?",
          "answer": "When connecting via RDP, you can configure display settings, sound, printers, and other devices. These settings are saved separately for each server."
        },
        {
          "id": "ssh_key",
          "question": "How to use SSH key?",
          "answer": "Select the 'Use SSH Key' option when connecting, then click 'Select SSH Key' and specify the path to your private key."
        },
        {
          "id": "troubleshooting",
          "question": "What to do with connection issues?",
          "answer": "Check your internet connection, ensure the server is available, and try restarting the application. If the problem persists, contact support."
        }
      ]
    },
    "security": {
      "title": "Security",
      "items": [
        {
          "id": "data_protection",
          "question": "How is my data protected?",
          "answer": "All connections are encrypted, secure protocols are used, and credentials are stored in encrypted form. We regularly update the security system."
        },
        {
          "id": "access_control",
          "question": "How is server access controlled?",
          "answer": "Access is granted only to authorized users. All connections are logged, and the administrator can view the connection history."
        }
      ]
    }
  },
